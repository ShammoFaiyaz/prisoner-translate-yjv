{
	"extends": "./tsconfig.node.json",
	"compilerOptions": {
		"jsx": "react-jsx",
		"target": "ES2020",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowJs": false,
		"strict": true,
		"baseUrl": ".",
		"types": ["vite/client"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
}


